{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "pink": "#F5C2E7",
        "lavender": "#B4BEFE",
        "blue":  "#89B4FA",
        "red": "#F38BA8",
        "text": "#45475A",
        "green": "#A6E3A1",
        "peach": "#FAB387",
        "teal": "#94E2D5",
"mauve": "#CBA6F7",
        "transparent": "transparent"
  },
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "newline": true,
      "segments": [
{
   "type": "shell",
   "style": "powerline",
   "powerline_symbol": "",
   "foreground": "p:text",
   "background": "p:teal",
   "properties": {
       "mapped_shell_names": {
           "pwsh": "\ue86c"
       }
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "template": " {{ .Path }}",
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "leading_powerline_symbol": "\ue0b6",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "template": " {{ .HEAD }}",
          "background": "p:peach",
          "foreground": "p:text",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "overflow": "hidden",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "p.green",
          "background": "p.transparent",
          "template": "{{ .FormattedMs }}",
          "properties": {
            "treshold": 5000
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{if gt .Code 0}}p:red{{end}}",
            "{{if eq .Code 0}}p:green{{end}}"
          ],
          "background": "p.transparent",
          "template": "❯"
        }
      ]
    }
  ],
  "transient_prompt": {
    "foreground_templates": [
      "{{if gt .Code 0}}p:red{{end}}",
      "{{if eq .Code 0}}p:green{{end}}"
    ],
    "background": "p.transparent",
    "template": "❯ "
  },
  "final_space": true,
  "version": 3
}
